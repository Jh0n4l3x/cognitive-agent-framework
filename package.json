{
  "name": "cognitive-agent-framework",
  "version": "1.0.0",
  "description": "A TypeScript cognitive framework with LLM integration, task management, and memory system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "cli": "ts-node src/cli.ts",
    "agent": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "example:research": "ts-node examples/research_agent.ts",
    "example:task": "ts-node examples/task_agent.ts",
    "example:yaml": "ts-node examples/yaml_agent_example.ts",
    "whatsapp": "ts-node src/integrations/whatsapp.ts",
    "laura": "ts-node src/integrations/whatsapp.ts",
    "demo:laura": "ts-node examples/laura_demo.ts"
  },
  "keywords": [
    "agent",
    "multi-agent",
    "llm",
    "ai",
    "typescript",
    "openai",
    "anthropic",
    "task-management",
    "memory"
  ],
  "author": "Jhon Alex M",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^1.0.2",
    "axios": "^1.6.5",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "langsmith": "^0.3.77",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.34.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.6",
    "@types/qrcode-terminal": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
